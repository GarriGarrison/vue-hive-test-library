{
  "name": "vue-hive-test-library",
  "private": false,
  "version": "0.0.9",
  "type": "module",
  "description": "UI kit for Vue 3",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "test": "exit 0"
  },
  "main": "./dist/vue-hive-test-library.umd.js",
  "module": "./dist/vue-hive-test-library.es.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/GarriGarrison/vue-hive-test-library"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-css-injected-by-js": "^3.1.2",
    "vue-tsc": "^1.8.3"
  },
  "author": {
    "name": "BDSM",
    "url": "https://github.com/hivebdsm"
  },
  "license": "MIT"
}
